@section scripts{
	<script type="text/javascript">
		$(window).on('load', function () {
			if (@ViewBag.LoginError == 1) {
				$('#LoginRuutu').modal('show');
			}
		});
	</script>
	@if (TempData["Message"] != null) //Alert kun sposti on lähtetty
	{
		<script type="text/javascript">
				$(document).ready(function () {
				alert("@TempData["Message"]");
			});
		</script>
	}
	<script type="text/javascript">
		$(window).on('load', function () {
			if (@ViewBag.LoginErrorYritys == 1) {
				$('#LoginRuutuYritys').modal('show');
			}
		});
	</script>
	@if (TempData["MessageYritys"] != null) //Alert kun sposti on lähtetty
	{
		<script type="text/javascript">
				$(document).ready(function () {
				alert("@TempData["MessageYritys"]");
				});
		</script>
	}
}

