<script>
	let toggle = button => {
		let element = document.getElementById("mytable");
		let hidden = element.getAttribute("hidden");

		if (hidden) {
			element.removeAttribute("hidden");
			button.innerText = "Hide table";
		} else {
			element.setAttribute("hidden", "hidden");
			button.innerText = "Show table";
		}
	}
</script>
